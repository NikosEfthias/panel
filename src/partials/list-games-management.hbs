{{#each games}}
  <tr>
    <td><b>{{name}}</b></td>
    <td></td>
    <td class="font-weight-bold">
      <div onclick="ctx.onGameEditClickHandler({{id}})" class="btn btn-info p-1"><span    style="font-size:1.3rem" class="cil-pencil btn-icon p-1"></span></div>
      <div onclick="ctx.onUserDeleteClickHandler('{{id}}')" class="btn btn-danger p-1"><span  style="font-size:1.3rem" class="cil-ban btn-icon p-1"></span></div>
    </td>
  </tr>
{{/each}}
