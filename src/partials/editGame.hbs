{{#with game}}
<div class="modal-content">
  <div class="modal-header">
    <h5 class="modal-title">Düzenle: {{name}}</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <div class="modal-body">
    <form>
      <span>Kart Alım süresi</span>
      <input type="number" class="form-control" placeholder="{{#if time}} {{time}}{{else}}Kart alım süresi{{/if}}"></br>
      <input type="number" class="form-control" placeholder="{{#if phone}} {{phone}}{{else}}kıl tüy{{/if}}"></br>
    </form>
  </div>
  <div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-dismiss="modal">Kapat</button>
    <button id='saveUserData' type="button" class="btn btn-primary" data-dismiss="modal">Kaydet</button>
  </div>
</div>
{{/with}}
