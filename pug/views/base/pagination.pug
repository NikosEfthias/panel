extends /default.pug

block view
  .animated.c-fade-in
    .c-card
      .c-card-header
        i.fa.fa-align-justify
        |  Pagination
        .c-card-header-actions
          a.c-card-header-action(href='http://coreui.io/docs/components/bootstrap-pagination/', target='_blank')
            small.c-text-muted docs
      .c-card-body
        nav(aria-label='Page navigation example')
          ul.c-pagination
            li.c-page-item
              a.c-page-link(href='#') Previous
            li.c-page-item
              a.c-page-link(href='#') 1
            li.c-page-item
              a.c-page-link(href='#') 2
            li.c-page-item
              a.c-page-link(href='#') 3
            li.c-page-item
              a.c-page-link(href='#') Next
