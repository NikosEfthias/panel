extends /default.pug

block view
  .animated.c-fade-in
    .c-card
      .c-card-header
        | Progress
        .c-card-header-actions
          a.c-card-header-action(href='http://coreui.io/docs/components/bootstrap-popover/popovers.html', target='_blank')
            small.c-text-muted docs
      .c-card-body
        .c-progress.c-mb-3
          .c-progress-bar(role='progressbar', aria-valuenow='0', aria-valuemin='0', aria-valuemax='100')
        .c-progress.c-mb-3
          .c-progress-bar(role='progressbar', style='width: 25%', aria-valuenow='25', aria-valuemin='0', aria-valuemax='100')
        .c-progress.c-mb-3
          .c-progress-bar(role='progressbar', style='width: 50%', aria-valuenow='50', aria-valuemin='0', aria-valuemax='100')
        .c-progress.c-mb-3
          .c-progress-bar(role='progressbar', style='width: 75%', aria-valuenow='75', aria-valuemin='0', aria-valuemax='100')
        .c-progress.c-mb-3
          .c-progress-bar(role='progressbar', style='width: 100%', aria-valuenow='100', aria-valuemin='0', aria-valuemax='100')
    .c-card
      .c-card-header
        | Progress
        small labels
      .c-card-body
        .c-progress.c-mb-3
          .c-progress-bar(role='progressbar', style='width: 25%;', aria-valuenow='25', aria-valuemin='0', aria-valuemax='100')
            | 25%
        .c-progress
          .c-progress-bar(role='progressbar', style='width: 75%;', aria-valuenow='75', aria-valuemin='0', aria-valuemax='100')
            | 75%
    .c-card
      .c-card-header
        | Progress
        small height
      .c-card-body
        .c-progress.c-mb-3(style='height: 3px;')
          .c-progress-bar(role='progressbar', style='width: 25%;', aria-valuenow='25', aria-valuemin='0', aria-valuemax='100')
        .c-progress(style='height: 30px;')
          .c-progress-bar(role='progressbar', style='width: 25%;', aria-valuenow='25', aria-valuemin='0', aria-valuemax='100')
    .c-card
      .c-card-header
        | Progress
        small backgrounds
      .c-card-body
        .c-progress.c-mb-3
          .c-progress-bar.c-bg-success(role='progressbar', style='width: 25%', aria-valuenow='25', aria-valuemin='0', aria-valuemax='100')
        .c-progress.c-mb-3
          .c-progress-bar.c-bg-info(role='progressbar', style='width: 50%', aria-valuenow='50', aria-valuemin='0', aria-valuemax='100')
        .c-progress.c-mb-3
          .c-progress-bar.c-bg-warning(role='progressbar', style='width: 75%', aria-valuenow='75', aria-valuemin='0', aria-valuemax='100')
        .c-progress
          .c-progress-bar.c-bg-danger(role='progressbar', style='width: 100%', aria-valuenow='100', aria-valuemin='0', aria-valuemax='100')
    .c-card
      .c-card-header
        | Progress 
        small multiple bars
      .c-card-body
        .c-progress
          .c-progress-bar(role='progressbar', style='width: 15%', aria-valuenow='15', aria-valuemin='0', aria-valuemax='100')
          .c-progress-bar.c-bg-success(role='progressbar', style='width: 30%', aria-valuenow='30', aria-valuemin='0', aria-valuemax='100')
          .c-progress-bar.c-bg-danger(role='progressbar', style='width: 20%', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
    .c-card
      .c-card-header
        | Progress 
        small striped
      .c-card-body
        .c-progress.c-mb-3
          .c-progress-bar.c-progress-bar-striped(role='progressbar', style='width: 10%', aria-valuenow='10', aria-valuemin='0', aria-valuemax='100')
        .c-progress.c-mb-3
          .c-progress-bar.c-progress-bar-striped.c-bg-success(role='progressbar', style='width: 25%', aria-valuenow='25', aria-valuemin='0', aria-valuemax='100')
        .c-progress.c-mb-3
          .c-progress-bar.c-progress-bar-striped.c-bg-info(role='progressbar', style='width: 50%', aria-valuenow='50', aria-valuemin='0', aria-valuemax='100')
        .c-progress.c-mb-3
          .c-progress-bar.c-progress-bar-striped.c-bg-warning(role='progressbar', style='width: 75%', aria-valuenow='75', aria-valuemin='0', aria-valuemax='100')
        .c-progress.c-mb-3
          .c-progress-bar.c-progress-bar-striped.c-bg-danger(role='progressbar', style='width: 100%', aria-valuenow='100', aria-valuemin='0', aria-valuemax='100')
    .c-card
      .c-card-header
        | Progress 
        small animated
      .c-card-body
        .c-progress
          .c-progress-bar.c-progress-bar-striped.c-progress-bar-animated(role='progressbar', aria-valuenow='75', aria-valuemin='0', aria-valuemax='100', style='width: 75%')
